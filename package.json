{
    "name": "luxe-staycations-api",
    "version": "1.0.0",
    "description": "Luxe Staycations API - Luxury Villa Rental Platform",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js"
    },
    "keywords": [
        "luxury",
        "villa",
        "rental",
        "booking",
        "api",
        "express",
        "supabase",
        "stripe"
    ],
    "author": "Luxe Staycations Team",
    "license": "MIT",
    "dependencies": {
        "@supabase/supabase-js": "^2.38.0",
        "express": "^4.18.2",
        "stripe": "^14.7.0",
        "zod": "^3.22.4",
        "cors": "^2.8.5",
        "helmet": "^7.1.0",
        "express-rate-limit": "^7.1.5",
        "dotenv": "^16.3.1",
        "uuid": "^9.0.1",
        "compression": "^1.7.4",
        "morgan": "^1.10.0",
        "winston": "^3.11.0",
        "redis": "^4.6.10",
        "bcryptjs": "^2.4.3",
        "jsonwebtoken": "^9.0.2",
        "multer": "^1.4.5-lts.1",
        "aws-sdk": "^2.1490.0",
        "nodemailer": "^6.9.7",
        "moment": "^2.29.4",
        "lodash": "^4.17.21"
    },
    "devDependencies": {
        "nodemon": "^3.0.2"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/luxe-staycations/api.git"
    },
    "bugs": {
        "url": "https://github.com/luxe-staycations/api/issues"
    },
    "homepage": "https://github.com/luxe-staycations/api#readme",
    "jest": {
        "testEnvironment": "node",
        "collectCoverageFrom": [
            "**/*.js",
            "!**/node_modules/**",
            "!**/coverage/**",
            "!**/jest.config.js"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 80,
                "functions": 80,
                "lines": 80,
                "statements": 80
            }
        }
    },
    "eslintConfig": {
        "extends": [
            "eslint:recommended"
        ],
        "env": {
            "node": true,
            "es2022": true,
            "jest": true
        },
        "parserOptions": {
            "ecmaVersion": 2022,
            "sourceType": "module"
        },
        "rules": {
            "no-console": "warn",
            "no-unused-vars": "error",
            "prefer-const": "error"
        }
    },
    "prettier": {
        "semi": true,
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 80,
        "tabWidth": 2
    }
}